package client;

import java.net.InetAddress;
import java.net.UnknownHostException;

import com.mkyong.ws.IThirdPartyServer;
import com.mkyong.ws.ThirdPartServerImplService;

public class Client {
	public static final String PORT = "9090";
	
	public static void main(String[] args) {
		 
		IThirdPartyServer helloService = (IThirdPartyServer) new ThirdPartServerImplService();
		
		IThirdPartyServer hello = helloService.;
 
		Thread d = new Thread(new NotificationListner(9090));
		
		d.start();
		
		hello.subscribe("http://157.169.100.132:9090/");
		
		System.out.println(hello.getHelloWorldAsString("mkyong"));
		
		try {
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
    }
}
